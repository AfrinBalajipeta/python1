import pandas as pd
marks = pd.Series([85,90,78,92,88],index = ["Alice","Bob","Charlie","David","Eva"])
print("Series:")
print(marks)
print("\nAccessing by index label:",marks["Bob"])
print("Accessing by position:",marks[2])
print("\nMultiple selection:\n", marks[["Alice","Eva"]])
print("\nMaximum Marks:", marks.max())
print("Minimum Marks:", marks.min())
print("Average Marks:", marks.mean())
print("\nDescribe:\n", marks.describe())
