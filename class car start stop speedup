class car:
  def __init__(self, make, model, year, speed):
    self.make = make
    self.model = model
    self.year = year
    self.speed = speed
  def start(self):
    print(f"{self.year} {self.make} {self.model} is starting . . . . .")
  def stop(self):
    print(f"{self.year} {self.make} {self.model} is stopping . . . . .")
  def speedup(self):
    print(f"{self.speed}")
  my_car.start()
  my_car.stop()
  speed.speedup()
