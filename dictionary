class Student:
    def __init__(self, name):
        self.name = name
s1 = Student("Alice")
s1.age = 21
s1.course = "Computer Science"
print(s1.__dict__)
print(getattr(s1, "name"))
print(getattr(s1, "age"))
print(getattr(s1, "course"))
print(getattr(s1, "marks", "Not Found"))
print(hasattr(s1, "age"))
print(hasattr(s1, "marks"))
del s1.age
print(s1.__dict__)
