import pandas as pd
df = pd.DataFrame({
 'Student_Name': ['Alice', 'Bob', 'Charlie'],
 'Marks': [60,70,90]
})
extra = pd.DataFrame({
 'Name': ['Alice', 'Bob', 'Charlie'],
 'Subject': ['Math', 'Science', 'English']
})
merged = pd.merge(df, extra,
left_on='Student_Name', right_on='Name',
how='left')
print(merged)
