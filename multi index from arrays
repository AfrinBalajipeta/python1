import pandas as pd
import numpy as np
arrays = [
 ['Class A', 'Class A', 'Class A', 'Class B',
'Class B', 'Class B'],
 ['Math', 'Science', 'English', 'Math',
'Science', 'English']
]
multi_index = pd.MultiIndex.from_arrays(arrays,
names=('Class', 'Subject'))
data = [85, 90, 78, 88, 92, 80]
df = pd.DataFrame(data, index=multi_index,
columns=['Score'])
print(df)
print(df.loc['Class A'])
print(df.loc[('Class B','Math')])
